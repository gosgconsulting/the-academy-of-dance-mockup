{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"settings": {
		"appearanceTools": true,
		"useRootPaddingAwareAlignments": true,
		"layout": {
			"contentSize": "1200px",
			"wideSize": "1400px"
		},
		"color": {
			"palette": [
				{
					"slug": "primary",
					"color": "hsl(220, 70%, 50%)",
					"name": "Primary"
				},
				{
					"slug": "primary-foreground",
					"color": "hsl(0, 0%, 98%)",
					"name": "Primary Foreground"
				},
				{
					"slug": "secondary",
					"color": "hsl(220, 14.3%, 95.9%)",
					"name": "Secondary"
				},
				{
					"slug": "secondary-foreground",
					"color": "hsl(220.9, 39.3%, 11%)",
					"name": "Secondary Foreground"
				},
				{
					"slug": "muted",
					"color": "hsl(220, 14.3%, 95.9%)",
					"name": "Muted"
				},
				{
					"slug": "muted-foreground",
					"color": "hsl(220, 8.9%, 46.1%)",
					"name": "Muted Foreground"
				},
				{
					"slug": "accent",
					"color": "hsl(220, 14.3%, 95.9%)",
					"name": "Accent"
				},
				{
					"slug": "accent-foreground",
					"color": "hsl(220.9, 39.3%, 11%)",
					"name": "Accent Foreground"
				},
				{
					"slug": "destructive",
					"color": "hsl(0, 84.2%, 60.2%)",
					"name": "Destructive"
				},
				{
					"slug": "destructive-foreground",
					"color": "hsl(0, 0%, 98%)",
					"name": "Destructive Foreground"
				},
				{
					"slug": "border",
					"color": "hsl(220, 13%, 91%)",
					"name": "Border"
				},
				{
					"slug": "input",
					"color": "hsl(220, 13%, 91%)",
					"name": "Input"
				},
				{
					"slug": "ring",
					"color": "hsl(220, 70%, 50%)",
					"name": "Ring"
				},
				{
					"slug": "background",
					"color": "hsl(0, 0%, 100%)",
					"name": "Background"
				},
				{
					"slug": "foreground",
					"color": "hsl(220.9, 39.3%, 11%)",
					"name": "Foreground"
				},
				{
					"slug": "card",
					"color": "hsl(0, 0%, 100%)",
					"name": "Card"
				},
				{
					"slug": "card-foreground",
					"color": "hsl(220.9, 39.3%, 11%)",
					"name": "Card Foreground"
				},
				{
					"slug": "popover",
					"color": "hsl(0, 0%, 100%)",
					"name": "Popover"
				},
				{
					"slug": "popover-foreground",
					"color": "hsl(220.9, 39.3%, 11%)",
					"name": "Popover Foreground"
				}
			],
			"gradients": [
				{
					"slug": "primary-gradient",
					"gradient": "linear-gradient(135deg, hsl(220, 70%, 50%) 0%, hsl(220, 70%, 40%) 100%)",
					"name": "Primary Gradient"
				},
				{
					"slug": "hero-gradient",
					"gradient": "linear-gradient(135deg, hsl(220, 70%, 50%) 0%, hsl(260, 70%, 50%) 100%)",
					"name": "Hero Gradient"
				}
			]
		},
		"typography": {
			"fontFamilies": [
				{
					"fontFamily": "Inter, system-ui, sans-serif",
					"slug": "primary",
					"name": "Primary Font"
				},
				{
					"fontFamily": "Georgia, serif",
					"slug": "secondary",
					"name": "Secondary Font"
				}
			],
			"fontSizes": [
				{
					"slug": "small",
					"size": "0.875rem",
					"name": "Small"
				},
				{
					"slug": "medium",
					"size": "1rem",
					"name": "Medium"
				},
				{
					"slug": "large",
					"size": "1.125rem",
					"name": "Large"
				},
				{
					"slug": "x-large",
					"size": "1.25rem",
					"name": "Extra Large"
				},
				{
					"slug": "xx-large",
					"size": "1.5rem",
					"name": "2X Large"
				},
				{
					"slug": "xxx-large",
					"size": "2rem",
					"name": "3X Large"
				},
				{
					"slug": "heading-small",
					"size": "1.875rem",
					"name": "Heading Small"
				},
				{
					"slug": "heading-medium",
					"size": "2.25rem",
					"name": "Heading Medium"
				},
				{
					"slug": "heading-large",
					"size": "3rem",
					"name": "Heading Large"
				},
				{
					"slug": "heading-xl",
					"size": "3.75rem",
					"name": "Heading XL"
				}
			]
		},
		"spacing": {
			"units": ["px", "em", "rem", "vh", "vw", "%"],
			"spacingSizes": [
				{
					"slug": "x-small",
					"size": "0.25rem",
					"name": "X-Small"
				},
				{
					"slug": "small",
					"size": "0.5rem",
					"name": "Small"
				},
				{
					"slug": "medium",
					"size": "1rem",
					"name": "Medium"
				},
				{
					"slug": "large",
					"size": "1.5rem",
					"name": "Large"
				},
				{
					"slug": "x-large",
					"size": "2rem",
					"name": "X-Large"
				},
				{
					"slug": "xx-large",
					"size": "3rem",
					"name": "2X-Large"
				},
				{
					"slug": "xxx-large",
					"size": "4rem",
					"name": "3X-Large"
				}
			]
		},
		"blocks": {
			"core/button": {
				"color": {
					"palette": [
						{
							"slug": "primary",
							"color": "hsl(220, 70%, 50%)",
							"name": "Primary Button"
						},
						{
							"slug": "secondary",
							"color": "hsl(220, 14.3%, 95.9%)",
							"name": "Secondary Button"
						}
					]
				}
			}
		}
	},
	"styles": {
		"color": {
			"background": "var(--wp--preset--color--background)",
			"text": "var(--wp--preset--color--foreground)"
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--primary)",
			"fontSize": "var(--wp--preset--font-size--medium)",
			"lineHeight": "1.6"
		},
		"spacing": {
			"blockGap": "var(--wp--preset--spacing--medium)"
		},
		"elements": {
			"heading": {
				"typography": {
					"fontWeight": "600",
					"lineHeight": "1.2"
				}
			},
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--heading-xl)"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--heading-large)"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--heading-medium)"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--heading-small)"
				}
			},
			"button": {
				"color": {
					"background": "var(--wp--preset--color--primary)",
					"text": "var(--wp--preset--color--primary-foreground)"
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)",
					"fontWeight": "500"
				},
				"border": {
					"radius": "6px"
				},
				"spacing": {
					"padding": {
						"top": "0.5rem",
						"right": "1rem",
						"bottom": "0.5rem",
						"left": "1rem"
					}
				}
			},
			"link": {
				"color": {
					"text": "var(--wp--preset--color--primary)"
				}
			}
		},
		"blocks": {
			"core/group": {
				"spacing": {
					"padding": {
						"top": "var(--wp--preset--spacing--large)",
						"bottom": "var(--wp--preset--spacing--large)"
					}
				}
			},
			"core/columns": {
				"spacing": {
					"blockGap": "var(--wp--preset--spacing--large)"
				}
			}
		}
	},
	"templateParts": [
		{
			"name": "header",
			"title": "Header",
			"area": "header"
		},
		{
			"name": "footer",
			"title": "Footer",
			"area": "footer"
		}
	]
}